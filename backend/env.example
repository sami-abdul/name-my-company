# Database Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key

# AI Services Configuration
OPENAI_API_KEY=sk-...
GROQ_API_KEY=gsk_...  # Optional: for LLaMA-3 models

# Domain Services Configuration
DOMAINR_API_KEY=your-domainr-api-key
RAPIDAPI_KEY=your-rapidapi-key  # Alternative for Domainr via RapidAPI

# Application Configuration
NODE_ENV=development
PORT=3000
CORS_ORIGIN=http://localhost:3000

# Usage Limits (per month)
FREE_TIER_LIMIT=2
MID_TIER_LIMIT=100
# PREMIUM_TIER_LIMIT can be a number or 'unlimited'
PREMIUM_TIER_LIMIT=unlimited
